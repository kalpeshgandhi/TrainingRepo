	<compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
	<deploymentStatus>Deployed</deploymentStatus>
	<enableActivities>false</enableActivities>
	<enableBulkApi>true</enableBulkApi>
	<enableFeeds>false</enableFeeds>
	<enableHistory>false</enableHistory>
	<enableReports>true</enableReports>
	<enableSharing>true</enableSharing>
	<enableStreamingApi>true</enableStreamingApi>
	<fields>
		<fullName>Bank_Account_Number__c</fullName>
		<description>Displays the Bank Account Number.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please enter the Bank Account Number.</inlineHelpText>
		<label>Bank Account Number</label>
		<length>25</length>
		<required>true</required>
		<trackTrending>false</trackTrending>
		<type>Text</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>Bank_Details__c</fullName>
		<description>Displays the bank Detail.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please enter the bank Detail.</inlineHelpText>
		<label>Bank Details</label>
		<length>100</length>
		<required>false</required>
		<trackTrending>false</trackTrending>
		<type>Text</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>Basics__c</fullName>
		<description>Display the Basic Salary.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please Enter Your Basic Salary.</inlineHelpText>
		<label>Basic</label>
		<precision>12</precision>
		<required>true</required>
		<scale>2</scale>
		<trackTrending>false</trackTrending>
		<type>Number</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>Credited_To__c</fullName>
		<description>Displays the Information Of Credited person.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please Enter the Employee ID On whose account the
			salary is credited.</inlineHelpText>
		<label>Credited To</label>
		<referenceTo>Employee__c</referenceTo>
		<relationshipLabel>Salarys</relationshipLabel>
		<relationshipName>Salarys</relationshipName>
		<relationshipOrder>0</relationshipOrder>
		<reparentableMasterDetail>false</reparentableMasterDetail>
		<trackTrending>false</trackTrending>
		<type>MasterDetail</type>
		<writeRequiresMasterRead>false</writeRequiresMasterRead>
	</fields>
	<fields>
		<fullName>Gross_Salary__c</fullName>
		<description>Displays the Gross Salary.</description>
		<externalId>false</externalId>
		<formula>Total_Earnings__c - PF_Deduction__c</formula>
		<formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
		<inlineHelpText>Please enter the Gross Salary.</inlineHelpText>
		<label>Gross Salary</label>
		<precision>18</precision>
		<required>false</required>
		<scale>0</scale>
		<trackTrending>false</trackTrending>
		<type>Number</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>HRA__c</fullName>
		<description>Displays the HRA Details.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please Enter the HRA Details.</inlineHelpText>
		<label>HRA</label>
		<precision>12</precision>
		<required>true</required>
		<scale>2</scale>
		<trackTrending>false</trackTrending>
		<type>Number</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>Mode_of_Payment__c</fullName>
		<description>Displays the Mode of Payment.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please Select The Mode of Payment.</inlineHelpText>
		<label>Mode of Payment</label>
		<picklist>
			<picklistValues>
				<fullName>RTGS</fullName>
				<default>false</default>
			</picklistValues>
			<picklistValues>
				<fullName>NEFT</fullName>
				<default>false</default>
			</picklistValues>
			<picklistValues>
				<fullName>Cheque</fullName>
				<default>false</default>
			</picklistValues>
			<picklistValues>
				<fullName>Cash</fullName>
				<default>false</default>
			</picklistValues>
			<sorted>false</sorted>
		</picklist>
		<trackTrending>false</trackTrending>
		<type>Picklist</type>
	</fields>
	<fields>
		<fullName>PF_Deduction__c</fullName>
		<description>Displays the Details of the PF Deduction.</description>
		<externalId>false</externalId>
		<inlineHelpText>Please Enter the Details of the PF.</inlineHelpText>
		<label>PF Deduction</label>
		<precision>12</precision>
		<required>true</required>
		<scale>2</scale>
		<trackTrending>false</trackTrending>
		<type>Number</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>Salary_Credit_Date__c</fullName>
		<externalId>false</externalId>
		<label>Salary Credit Date</label>
		<required>true</required>
		<trackTrending>false</trackTrending>
		<type>Date</type>
	</fields>
	<fields>
		<fullName>Special_Allowance__c</fullName>
		<externalId>false</externalId>
		<label>Special Allowance</label>
		<precision>12</precision>
		<required>true</required>
		<scale>2</scale>
		<trackTrending>false</trackTrending>
		<type>Number</type>
		<unique>false</unique>
	</fields>
	<fields>
		<fullName>Total_Earnings__c</fullName>
		<externalId>false</externalId>
		<formula>Basics__c + HRA__c + Special_Allowance__c</formula>
		<formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
		<label>Total Earnings</label>
		<precision>18</precision>
		<required>false</required>
		<scale>2</scale>
		<trackTrending>false</trackTrending>
		<type>Number</type>
		<unique>false</unique>
	</fields>
	<label>Salary</label>
	<nameField>
		<displayFormat>SAL-{000000}</displayFormat>
		<label>Salary ID</label>
		<type>AutoNumber</type>
	</nameField>
	<pluralLabel>Salarys</pluralLabel>
	<searchLayouts />
	<sharingModel>ControlledByParent</sharingModel>
	<validationRules>
		<fullName>BAnk_account</fullName>
		<active>true</active>
		<errorConditionFormula>ISCHANGED( Bank_Account_Number__c )</errorConditionFormula>
		<errorMessage>Bank Account No can Not Be Chaged</errorMessage>
	</validationRules>
	<validationRules>
		<fullName>Banking</fullName>
		<active>true</active>
		<errorConditionFormula>ISBLANK( Bank_Details__c )</errorConditionFormula>
		<errorDisplayField>Bank_Details__c</errorDisplayField>
		<errorMessage>Bank detail field cant be blank.</errorMessage>
	</validationRules>
	<validationRules>
		<fullName>Salary_Record</fullName>
		<active>true</active>
		<description>It shows how salary record cant be generated if Employee
			is inactive.</description>
		<errorConditionFormula>IF( Credited_To__r.Is_Active__c = False,True,
			False)</errorConditionFormula>
		<errorMessage>Salary cannot be processed for inactive employees.</errorMessage>
	</validationRules>
</CustomObject>
